   <string notr="true">
   
/* Estilos generales */
QWidget {
	font-family: 'Segoe UI', Arial, sans-serif;
	background-color: #f8f9fa;
}

/* 1. Estilo para el Banner Superior (Great progress today...) */
QFrame#mainProgressBanner {
	background-color: #4A90E2; /* Azul del banner */
	border-radius: 8px;
	padding: 15px;
}

/* Selector CORREGIDO: Solo aplicamos blanco a los QLabels específicos del banner */
QFrame#mainProgressBanner &gt; QHBoxLayout &gt; QVBoxLayout QLabel {
    color: white;
}

/* Estilos de fuente específicos */
QLabel#bannerGreeting {
	font-size: 20px;
	font-weight: bold;
}

QLabel#bannerSubtext {
	font-size: 14px;
}

QLabel#bannerPercentage {
	font-size: 40px;
	font-weight: 300;
}

/* 2. Contenedores de tarjetas de progreso */
QFrame#progressCard {
	background-color: white;
	border-radius: 8px; /* Ligeramente más pequeño */
	border: 1px solid #e0e0e0;
	padding: 15px;
	margin-top: 10px; /* Separación de las tarjetas del banner */
}

QLabel#cardTitle {
	font-size: 16px;
	font-weight: bold;
	color: #2c3e50;
	margin-bottom: 5px;
}

QLabel#progressText {
	font-size: 13px;
	color: #7f8c8d;
	margin-top: 5px;
}

QLabel#percentage {
	font-size: 14px; /* Más pequeño para las tarjetas */
	font-weight: bold;
}

/* 3. Barras de progreso personalizadas */
QFrame#progressBarBackground {
	background-color: #ecf0f1;
	border-radius: 10px;
	border: none;
	height: 20px;
}

/* Color para la barra de progreso DIARIO (Azul) */
QFrame#dailyProgressBarFill {
	background-color: #4A90E2; /* Azul fuerte para el 50% */
	border-radius: 10px;
	border: none;
}

/* Color para la barra de progreso de RACHA (Verde) */
QFrame#streakProgressBarFill {
	background-color: #7ED321; /* Verde de la imagen 2 */
	border-radius: 10px;
	border: none;
}</string>
